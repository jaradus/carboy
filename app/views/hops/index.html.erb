<table>
  <thead>
    <tr>
      <% @hops_header.each do |h| %>
      <td><%= h.humanize %></td>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @hops.each do |hop| %>
    <tr>
    <!-- TODO: Need to remove ids, etc from the the hops -->
      <% hop.attributes.each do |k,v| %>
      <td><%= v %></td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>